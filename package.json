{
  "name": "docsign-ts-client",
  "description": "A client for the Inleed Docsign API",
  "author": "Jonatan Hallenberg",
  "license": "MIT",
  "homepage": "https://www.sprinto.se",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonatan.hallenberg/docsign-ts-client.git"
  },
  "bugs": {
    "url": "https://github.com/jonatan.hallenberg/docsign-ts-client/issues"
  },
  "keywords": [
    "docsign",
    "inleed",
    "api",
    "client",
    "typescript"
  ],
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/docsign-ts-client.umd.cjs",
  "module": "./dist/docsign-ts-client.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/docsign-ts-client.js",
      "require": "./dist/docsign-ts-client.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "release": "npm run test && npm version patch && npm publish"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "nock": "^13.3.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  }
}
